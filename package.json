    "@types/koa__cors": "^2.2.2",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.12",
    "build-error-notifier": "^1.0.0",
    "core-decorators": "^0.20.0",
    "expo": "^25.0.0",
